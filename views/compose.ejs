<%- include("partials/header") -%>

<h1>Compose</h1>

<form action="/compose" method="post">
  <div class="form-group">
    <label for="basic-url">Title</label>
    <input class="form-control" type="text" name="postTitle">
    <label for="basic-url">Post</label>
    <textarea name="form-control" name="postBody" rows="5" cols="160"></textarea>
  </div>
  <button class="btn btn-primary" type="submit" name="button">Publish</button>
</form>

